{
  "name": "bitwig-multisamplegen",
  "version": "0.0.1",
  "description": "Generate multisample xml file from samples ",
  "author": "guillaumearm",
  "license": "MIT",
  "homepage": "https://github.com/guillaumearm/bitwig-multisamplegen",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guillaumearm/bitwig-multisamplegen.git"
  },
  "bin": {
    "create-ts-cli-app": "./dist/index.js"
  },
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.ts",
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "CLI",
    "typescript"
  ],
  "scripts": {
    "clean": "rm dist/*",
    "test": "jest --coverage",
    "lint": "run-s lint:js lint:prettier",
    "lint-staged:js": "eslint --fix --cache --ext .ts,.tsx ./src",
    "lint:js": "eslint --fix --cache --ext .ts,.tsx ./src",
    "lint:prettier": "prettier --check --write \"**/*\" --end-of-line auto",
    "start": "microbundle-crl watch --no-compress --format cjs",
    "build": "microbundle-crl --no-compress --format cjs",
    "prettier": "prettier -c --write \"**/*\"",
    "release": "git fetch --tags && npx standard-version",
    "release:alpha": "npm run release -- --prerelease alpha",
    "commit": "lint-staged && npx git-cz",
    "prepublishOnly": "run-s lint build"
  },
  "lint-staged": {
    "**/*.{ts}": "npm run lint-staged:js",
    "**/*.{ts,md,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "execa": "4.1.0",
    "inquirer": "7.3.3",
    "ora": "5.1.0",
    "p-each-series": "2.1.0",
    "ramda": "^0.28.0",
    "yargs": "^17.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-typescript": "7.10.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@types/execa": "2.0.0",
    "@types/inquirer": "7.3.1",
    "@types/jest": "26.0.14",
    "@types/ora": "3.2.0",
    "@types/p-each-series": "1.0.1",
    "@types/ramda": "^0.28.3",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "babel-jest": "26.3.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "4.3.0",
    "jest": "26.4.2",
    "lint-staged": "^10.0.0",
    "microbundle-crl": "^0.13.11",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2",
    "typescript": "4.0.3"
  },
  "files": [
    "dist"
  ]
}
